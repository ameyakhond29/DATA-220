   "source": [
    "import numpy as np\n",
    "\n",
    "x = np.array([1, 2, 3, 4])\n",
    "y = np.array([2, 3, 5, 7])\n",
    "\n",
    "x_interp = 2.5\n",
    "\n",
    "y_interp = np.interp(x_interp, x, y)\n",
    "\n",
    "print(\"Interpolated value at x = 2.5 is:\", y_interp)\n"
   ]
